{
    "name": "soft1c/laravel-date-localization",
    "license": "MIT",
    "keywords": ["laravel", "date", "time", "datetime", "i18n", "translation", "carbon"],
    "homepage": "https://github.com/1csoft/laravel-date-localization",
    "require": {
        "php": "^7.2.5",
        "laravel/framework": "^6.0|^7.0",
        "nesbot/carbon": "^1.0|^2.0"
    },
    "authors": [
        {
            "name": "Stanislav Dremin",
            "email": "dres@1c.ru"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Soft1c\\Date\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Soft1c\\Date\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Soft1c\\Date\\DateServiceProvider"
            ],
            "aliases": {
                "SoftDate": "Soft1c\\Date\\Date"
            }
        }
    }
}
